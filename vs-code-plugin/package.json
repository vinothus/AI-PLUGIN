{
  "name": "enterprise-ai-plugin-vscode",
  "displayName": "Enterprise AI Plugin",
  "description": "Enterprise-grade AI coding assistant with data protection and custom plugin support",
  "version": "1.0.0",
  "publisher": "enterprise-ai-plugin",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "AI",
    "Other",
    "Programming Languages",
    "Snippets",
    "Testing"
  ],
  "keywords": [
    "ai",
    "coding",
    "assistant",
    "enterprise",
    "security",
    "compliance"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:enterprise-ai-plugin.start",
    "onCommand:enterprise-ai-plugin.configure"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "enterprise-ai-plugin.start",
        "title": "Enterprise AI Plugin: Start AI Assistant",
        "category": "Enterprise AI Plugin"
      },
      {
        "command": "enterprise-ai-plugin.configure",
        "title": "Enterprise AI Plugin: Configure Settings",
        "category": "Enterprise AI Plugin"
      },
      {
        "command": "enterprise-ai-plugin.install-plugin",
        "title": "Enterprise AI Plugin: Install Plugin",
        "category": "Enterprise AI Plugin"
      },
      {
        "command": "enterprise-ai-plugin.security-audit",
        "title": "Enterprise AI Plugin: Security Audit",
        "category": "Enterprise AI Plugin"
      }
    ],
    "configuration": {
      "title": "Enterprise AI Plugin",
      "properties": {
        "enterpriseAiPlugin.serverUrl": {
          "type": "string",
          "default": "",
          "description": "Enterprise AI Plugin server URL"
        },
        "enterpriseAiPlugin.authType": {
          "type": "string",
          "enum": [
            "sso",
            "api-key",
            "oauth"
          ],
          "default": "sso",
          "description": "Authentication type"
        },
        "enterpriseAiPlugin.dataRetention": {
          "type": "number",
          "default": 90,
          "description": "Data retention period in days"
        },
        "enterpriseAiPlugin.encryptionLevel": {
          "type": "string",
          "enum": [
            "standard",
            "enhanced",
            "military"
          ],
          "default": "enhanced",
          "description": "Encryption level"
        },
        "enterpriseAiPlugin.complianceMode": {
          "type": "string",
          "enum": [
            "gdpr",
            "soc2",
            "hipaa",
            "custom"
          ],
          "default": "gdpr",
          "description": "Compliance mode"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "enterprise-ai-plugin",
          "title": "Enterprise AI Plugin",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "enterprise-ai-plugin": [
        {
          "id": "enterprise-ai-plugin.chat",
          "name": "AI Chat"
        },
        {
          "id": "enterprise-ai-plugin.plugins",
          "name": "Plugins"
        },
        {
          "id": "enterprise-ai-plugin.security",
          "name": "Security"
        },
        {
          "id": "enterprise-ai-plugin.audit",
          "name": "Audit Log"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.84.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vsce": "^2.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.0",
    "@google/generative-ai": "^0.2.0",
    "@types/chokidar": "^1.7.5",
    "axios": "^1.6.0",
    "chokidar": "^4.0.3",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.0",
    "node-forge": "^1.3.0",
    "openai": "^4.20.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "ws": "^8.14.0"
  }
}
